{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "coverage/"
    ],
    "watch": [
        "src/"
    ],
    "execMap": {
        "ts": "ts-node --files ./src/index.ts"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
}